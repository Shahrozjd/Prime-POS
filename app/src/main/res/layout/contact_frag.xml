        android:gravity="left"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="24dp"
            android:text="Development Team"
            android:textColor="#FFF"


            />
    </LinearLayout>

    <LinearLayout
