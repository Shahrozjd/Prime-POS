                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:src="@drawable/ic_call_black_24dp" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
